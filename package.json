{
    "name": "node-epicsgg",
    "version": "2.3.8",
    "description": "An interface to communicate with Epics.gg API",
    "keywords": [
        "epics.gg",
        "epics"
    ],
    "author": {
        "url": "https://clmntw.fr",
        "email": "git@clmntw.fr",
        "name": "Roguyt"
    },
    "contributors": [
        {
            "url": "https://sherlox.io",
            "email": "me@sherlox.io",
            "name": "Pierre Cavin"
        }
    ],
    "homepage": "https://github.com/Roguyt/node-epicsgg#readme",
    "repository": {
        "url": "https://github.com/Roguyt/node-epicsgg.git",
        "type": "git"
    },
    "bugs": {
        "url": "https://github.com/Roguyt/node-epicsgg/issues",
        "email": "git@clmntw.fr"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "scripts": {
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "lint": "eslint '{src,test}/**/*.{ts,tsx}' --fix",
        "format": "prettier --write 'src/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}'",
        "build": "tsc -p tsconfig.build.json",
        "build:watch": "tsc -w -p tsconfig.build.json",
        "build:docs": "typedoc --out ./docs --tsconfig tsconfig.build.json --mode file --excludeExternals --includeDeclarations --excludePrivate --excludeNotExported --theme minimal",
        "prepack": "yarn run build",
        "release": "semantic-release"
    },
    "main": "lib/index.js",
    "typings": "lib/index.d.ts",
    "license": "MIT",
    "files": [
        "lib/"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "eslint \"{src,test}/**/*.ts\" --fix && pretty-quick --staged"
        }
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": ".",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "./coverage",
        "testEnvironment": "node",
        "roots": [
            "<rootDir>/src/"
        ]
    },
    "devDependencies": {
        "@semantic-release/changelog": "^5.0.1",
        "@semantic-release/commit-analyzer": "^8.0.1",
        "@semantic-release/exec": "^5.0.0",
        "@semantic-release/git": "^9.0.0",
        "@semantic-release/github": "^7.0.5",
        "@semantic-release/npm": "^7.0.5",
        "@semantic-release/release-notes-generator": "^9.0.1",
        "@types/debug": "^4.1.5",
        "@types/jsonwebtoken": "^8.3.9",
        "@types/node": "^12.0.12",
        "@types/socket.io": "^2.1.2",
        "@typescript-eslint/eslint-plugin": "3.0.2",
        "@typescript-eslint/parser": "3.0.2",
        "eslint": "7.1.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "conventional-changelog-insurgent": "^6.1.1",
        "husky": "^4.2.5",
        "prettier": "^2.0.5",
        "pretty-quick": "^3.0.0",
        "resolve": "^1.11.1",
        "semantic-release": "^17.0.7",
        "ts-jest": "26.1.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
        "tsconfig-paths": "^3.9.0",
        "typedoc": "^0.19.0",
        "typescript": "^3.9.9",
        "@types/jest": "25.2.3",
        "jest": "26.0.1"
    },
    "dependencies": {
        "axios": "^0.21.0",
        "debug": "^4.1.1",
        "https-proxy-agent": "^5.0.0",
        "jsonwebtoken": "^8.5.1",
        "socket.io-client": "^2.2.0"
    }
}
